<% if @feed_items.any? %>
  <div class="row">
    <div class="small-12 columns">
      <%= will_paginate @feed_items, renderer: FoundationPagination::Rails %>
      <ul>
        <%= render partial: 'shared/feed_item', collection: @feed_items %>
      </ul>
      <%= will_paginate @feed_items, renderer: FoundationPagination::Rails %>
    </div>
  </div>
<% end %>